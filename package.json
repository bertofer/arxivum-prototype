{
  "name": "archivum-prototype",
  "version": "0.1.0",
  "description": "This is a prototype to try mixing some technologies together, for future use in the Archivum project",
  "main": "index.js",
  "scripts": {
    "build": "browserify client/index.js -o client/build.js",
    "start": "node .",
    "build-start": "npm run build && npm run start",
    "watch": "nodemon --exec 'npm run build-start' --ignore client/build.js"
  },
  "author": "Alberto Fernandez (Bertofer)",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^13.1.0",
    "nodemon": "^1.10.2",
    "standard": "^8.0.0"
  },
  "dependencies": {
    "bittorrent-tracker": "^8.0.11",
    "co-busboy": "^1.3.1",
    "create-torrent": "^3.24.7",
    "jquery": "^3.1.0",
    "koa": "^1.2.4",
    "koa-range": "^0.2.1",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "parse-torrent": "^5.8.1",
    "stream-http": "^2.4.0",
    "webtorrent": "^0.96.4"
  }
}
